<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_width="fill_parent"
	android:layout_height="fill_parent"
	android:orientation="vertical">

	<FrameLayout
		android:layout_height="wrap_content"
		android:layout_width="match_parent"
		android:orientation="horizontal"
		android:gravity="left|center"
		android:layout_margin="4dp">

		<Button
			android:id="@+id/item_geren"
			android:layout_height="50dp"
			android:text="@string/personal_details"
			android:textAppearance="?android:attr/textAppearanceMedium"
			android:layout_width="match_parent"
			android:layout_weight="1.0"
			android:layout_gravity="center"
			android:gravity="left|center"
			android:background="@drawable/list_item0"/>

		<ImageView
			android:layout_height="wrap_content"
			android:layout_width="wrap_content"
			android:layout_gravity="right|center"
			android:layout_marginRight="10dp"
			android:src="@drawable/listitem_to"/>

	</FrameLayout>

	<FrameLayout
		android:layout_height="wrap_content"
		android:layout_width="match_parent"
		android:orientation="horizontal"
		android:gravity="left|center"
		android:layout_margin="4dp">

		<Button
			android:id="@+id/item_myrefer"
			android:layout_height="50dp"
			android:text="@string/my_consult"
			android:textAppearance="?android:attr/textAppearanceMedium"
			android:layout_width="match_parent"
			android:layout_weight="1.0"
			android:layout_gravity="center"
			android:gravity="left|center"
			android:background="@drawable/list_item0"/>

		<ImageView
			android:layout_height="wrap_content"
			android:layout_width="wrap_content"
			android:layout_gravity="right|center"
			android:layout_marginRight="10dp"
			android:src="@drawable/listitem_to"/>

	</FrameLayout>

	<FrameLayout
		android:layout_height="wrap_content"
		android:layout_width="match_parent"
		android:orientation="horizontal"
		android:gravity="left|center"
		android:layout_margin="4dp">

		<Button
			android:id="@+id/item_sendtype"
			android:layout_height="50dp"
			android:text="@string/send_type"
			android:textAppearance="?android:attr/textAppearanceMedium"
			android:layout_width="match_parent"
			android:layout_weight="1.0"
			android:layout_gravity="center"
			android:gravity="left|center"
			android:background="@drawable/list_item0"/>

		<ImageView
			android:layout_height="wrap_content"
			android:layout_width="wrap_content"
			android:layout_gravity="right|center"
			android:layout_marginRight="10dp"
			android:src="@drawable/listitem_to"/>

	</FrameLayout>

	<FrameLayout
		android:layout_height="wrap_content"
		android:layout_width="match_parent"
		android:orientation="horizontal"
		android:gravity="left|center"
		android:layout_margin="4dp">

		<Button
			android:id="@+id/item_about"
			android:layout_height="50dp"
			android:text="@string/about_us"
			android:textAppearance="?android:attr/textAppearanceMedium"
			android:layout_width="match_parent"
			android:layout_weight="1.0"
			android:layout_gravity="center"
			android:gravity="left|center"
			android:background="@drawable/list_item0"/>

		<ImageView
			android:layout_height="wrap_content"
			android:layout_width="wrap_content"
			android:layout_gravity="right|center"
			android:layout_marginRight="10dp"
			android:src="@drawable/listitem_to"/>

	</FrameLayout>

</LinearLayout>

